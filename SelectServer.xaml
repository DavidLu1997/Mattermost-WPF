<Window x:Class="Mattermost_WPF.SelectServer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mattermost_WPF"
        mc:Ignorable="d"
        Title="Mattermost" Height="150" Width="300">
    <Grid>
        <Button x:Name="button" Content="Proceed" HorizontalAlignment="Left" Margin="115,80,0,0" VerticalAlignment="Top" Width="70"/>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="50,40,0,0" VerticalAlignment="Top" Width="200"/>
        <Label x:Name="label" Content="Select Server" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Width="81"/>
    </Grid>
</Window>
